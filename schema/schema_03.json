{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Detalles de empresa",
  "type": "object",
  "properties": {
    "IDENTIFICADOR_UJ": {
      "type": "string",
      "pattern": "^[a-zA-Z0-9]+$" 
    },
    "NOMBRE_EMPRESA_RAZON_SOCIAL": { "type": "string" },
    "NIF": { "type": "string" },
    "GRUPO": { "type": "string" },
    "NUM_ESTAB_CAE": { "type": "integer", "minimum": 0 },
    "NUM_ESTAB_ESTADO": { "type": "integer", "minimum": 0 },
    
    "Provincia_o_Territorio_Historico_empresa": { "type": "string" },
    "Codigo_Provincia_TH_empresa": { "type": "string" },
    "Municipio_empresa": { "type": "string" },
    "Codigo_Municipio_empresa": { "type": "string" },
    "Entidad_empresa": { "type": "string" },
    "Codigo_Entidad_empresa": { "type": "string" },
    
    "Direccion_postal_empresa": { "type": "string" },
    "Complemento_direccion_postal_empresa": { "type": "string" },
    "Codigo_postal_empresa": { "type": "string", "pattern": "^[0-9]{5}$" },
    
    "Codigo_Calle_empresa": { "type": "string" },
    "Codigo_Bloque_empresa": { "type": "string" },
    "Codigo_Numero_empresa": { "type": "string" },
    "Codigo_Bis_empresa": { "type": "string" },
    "Codigo_Escalera_empresa": { "type": "string" },
    "Codigo_Piso_empresa": { "type": "string" },
    "Codigo_Mano_empresa": { "type": "string" },
    "Codigo_Puerta_empresa": { "type": "string" },
    
    "Primer_telefono_empresa": { "type": "string", "pattern": "^[0-9\\s\\-\\+]{9,15}$" },
    "Segundo_telefono_empresa": { "type": "string", "pattern": "^[0-9\\s\\-\\+]{9,15}$" },
    "Correo_electronico_empresa": { "type": "string", "format": "email" },
    "Direccion_sitio_web": { "type": "string", "format": "uri" },
    
    "Actividad_principal_CNAE_09": { "type": "string" },
    "Actividad_principal_CNAE_25": { "type": "string" },
    
    "EMPLEO_CAE": { "type": "integer", "minimum": 0 }, //Esta variable se presenta en rangos por lo que habria que conocer los rangos
    // Para poder definirlo en forma de enum.
    "EMPLEO_Estado": { "type": "integer", "minimum": 0 },
    "Mujeres_empleadas_CAE": { "type": "integer", "minimum": 0 },
    "Personas_asalariadas_CAE": { "type": "integer", "minimum": 0 },
    "Mujeres_asalariadas_CAE": { "type": "integer", "minimum": 0 }
  },
  "required": [
    "IDENTIFICADOR_UJ",
    "NOMBRE_EMPRESA_RAZON_SOCIAL",
    "NIF",
    "GRUPO",
    "NUM_ESTAB_CAE",
    "NUM_ESTAB_ESTADO",
    "Provincia_o_Territorio_Historico_empresa",
    "Codigo_Provincia_TH_empresa",
    "Municipio_empresa",
    "Codigo_Municipio_empresa",
    "Direccion_postal_empresa",
    "Complemento_direccion_postal_empresa",
    "Codigo_postal_empresa",
    "Primer_telefono_empresa",
    "Correo_electronico_empresa",
    "Actividad_principal_CNAE_09"
  ],
  "additionalProperties": false
}
